<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>entry</key>
  <dict>
    <key>cell</key><string>PSLinkCell</string>
    <key>label</key><string>QuickTranslate</string>
  </dict>
  <key>title</key><string>QuickTranslate</string>
  <key>items</key>
  <array>
    <dict>
      <key>cell</key><string>PSSwitchCell</string>
      <key>label</key><string>Aktiviert</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>enabled</string>
      <key>default</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSGroupCell</string>
      <key>label</key><string>Anbieter</string>
      <key>footerText</key><string>API Keys kannst du jetzt sicher eingeben (öffnet Editor-Ansicht).</string>
    </dict>

    <dict>
      <key>cell</key><string>PSMultiValueCell</string>
      <key>label</key><string>Primär</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>providerPrimary</string>
      <key>default</key><string>libre</string>
      <key>values</key>
      <array>
        <string>libre</string><string>deepl</string><string>microsoft</string><string>google</string>
      </array>
      <key>titles</key>
      <array>
        <string>LibreTranslate</string><string>DeepL</string><string>Microsoft</string><string>Google (Cloud v2)</string>
      </array>
    </dict>

    <dict>
      <key>cell</key><string>PSMultiValueCell</string>
      <key>label</key><string>Fallback 1</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>providerFallback1</string>
      <key>default</key><string>deepl</string>
      <key>values</key>
      <array>
        <string>libre</string><string>deepl</string><string>microsoft</string><string>google</string><string>off</string>
      </array>
      <key>titles</key>
      <array>
        <string>LibreTranslate</string><string>DeepL</string><string>Microsoft</string><string>Google</string><string>Aus</string>
      </array>
    </dict>

    <dict>
      <key>cell</key><string>PSMultiValueCell</string>
      <key>label</key><string>Fallback 2</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>providerFallback2</string>
      <key>default</key><string>microsoft</string>
      <key>values</key>
      <array>
        <string>libre</string><string>deepl</string><string>microsoft</string><string>google</string><string>off</string>
      </array>
      <key>titles</key>
      <array>
        <string>LibreTranslate</string><string>DeepL</string><string>Microsoft</string><string>Google</string><string>Aus</string>
      </array>
    </dict>

    <dict>
      <key>cell</key><string>PSGroupCell</string>
      <key>label</key><string>Sprache</string>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>Zielsprache</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>targetLang</string>
      <key>placeholder</key><string>de</string>
      <key>isEditable</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSGroupCell</string>
      <key>label</key><string>LibreTranslate</string>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>Server URL</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>ltServer</string>
      <key>placeholder</key><string>https://translate.cutie.dating</string>
      <key>isEditable</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>api_key (optional)</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>ltApiKey</string>
      <key>placeholder</key><string>leer</string>
      <key>isEditable</key><true/>
      <key>isSecure</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSGroupCell</string>
      <key>label</key><string>DeepL</string>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>Auth Key</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>deeplKey</string>
      <key>placeholder</key><string>deepl key</string>
      <key>isEditable</key><true/>
      <key>isSecure</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSSwitchCell</string>
      <key>label</key><string>DeepL Pro</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>deeplPro</string>
      <key>default</key><false/>
    </dict>

    <dict>
      <key>cell</key><string>PSGroupCell</string>
      <key>label</key><string>Microsoft</string>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>Subscription Key</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>msKey</string>
      <key>placeholder</key><string>azure key</string>
      <key>isEditable</key><true/>
      <key>isSecure</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>Region (optional)</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>msRegion</string>
      <key>placeholder</key><string>westeurope</string>
      <key>isEditable</key><true/>
    </dict>

    <dict>
      <key>cell</key><string>PSGroupCell</string>
      <key>label</key><string>Google</string>
    </dict>

    <dict>
      <key>cell</key><string>PSLinkCell</string>
      <key>label</key><string>API Key</string>
      <key>detail</key><string>PSEditTextViewController</string>
      <key>defaults</key><string>com.hombergerkurde.quicktranslate</string>
      <key>key</key><string>googleKey</string>
      <key>placeholder</key><string>google key</string>
      <key>isEditable</key><true/>
      <key>isSecure</key><true/>
    </dict>

  </array>
</dict>
</plist>
